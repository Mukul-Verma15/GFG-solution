  int n=arr.size();
        vector<int> temp;
        d=d%10
        
        for(int i=d;i<n;i++){
            temp.push_back(arr[i]);
        }
        
        for(int i=0;i<d;i++){
            temp.push_back(arr[i]);
        }
        
        for(int i=0;i<n;i++){
            arr[i]=temp[i];
        }
