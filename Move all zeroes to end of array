class Solution{
public:
	void pushZerosToEnd(int arr[], int n) {
	    // code here
	auto count=0;
    
    for(int i=0;i<n;i++)
    {
        //check for non zros 
        if(arr[i]!=0)
        {
            //move zero to end
            swap(arr[i],arr[count]);
            count++;
        }
    }
	}
};
