vector<int> v;
        int first=-1,last=-1;
        int count=0;
        for(int i=0;i<n;i++){
            if(arr[i]==x && count==0){
                first=i;
                count++;
            }
            
            if(arr[i]==x){
                last=i;
            }
        }
        v.push_back(first);
        v.push_back(last);
        return v;
